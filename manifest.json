{
	"manifest_version": 2,
	"name": "Chegg Device Count Bypass",
	"version": "0.0.2",
	"description": "Removes Chegg's device registration limit pop-up and adds back scrolling on any page.",
	"homepage_url": "https://github.com/ayserjamshidi/Chegg-Device-Bypass",
	"content_scripts": [
		{
			"matches": ["*://*.chegg.com/*"],
			"js": ["src/main.js"]
		}
	],
	"browser_action": {
		"default_icon": "logo.png",
		"default_title": "Chegg"
	},
	"icons": {
		"16": "logo.png",
		"48": "logo.png"
	}
}
